{
  "name": "innerself-hn",
  "version": "1.0.0",
  "description": "innerself version of hacker news",
  "main": "index.js",
  "scripts": {
    "rollup": "rollup -c ./rollup.config.js",
    "test":
      "prettier --single-quote --write './src/**/*.ts' && tslint --project ./ --fix --type-check './src/**/*.ts'",
    "start": "npm run rollup -- -w",
    "build": "export NODE_ENV='production' && npm run rollup",
    "sync": "aws s3 sync ./public-s3/ s3://innerself-hn.com",
    "deploy": "npm run build && npm run sync"
  },
  "keywords": ["innerself", "hacker", "news", "hn"],
  "author": "bsouthga@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "clean-css": "^4.1.8",
    "prettier": "^1.7.0",
    "rollup": "^0.49.3",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-typescript2": "^0.5.2",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.5.0",
    "typescript": "^2.5.2",
    "uglify-es": "^3.1.0"
  },
  "dependencies": {
    "innerself": "^0.1.1"
  }
}
