{
  "name": "innerself-hn",
  "version": "1.0.0",
  "description": "innerself version of hacker news",
  "main": "index.js",
  "scripts": {
    "rollup": "rollup -c ./rollup.config.js",
    "tsc": "tsc",
    "test": "prettier --single-quote --write './src/**/*.ts' && tslint --project ./ --fix './src/**/*.ts'",
    "start": "npm run rollup -- -w",
    "build": "export NODE_ENV='production' && npm run rollup",
    "sync": "aws s3 sync ./public-s3/ s3://innerself-hn.com",
    "deploy": "npm run build && npm run sync"
  },
  "keywords": [
    "innerself",
    "hacker",
    "news",
    "hn"
  ],
  "author": "bsouthga@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "10.0.0",
    "csso": "4.2.0",
    "prettier": "2.2.0",
    "rollup": "2.33.3",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-serve": "1.1.0",
    "rollup-plugin-typescript2": "0.29.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.1.2",
    "uglify-es": "3.3.9"
  },
  "dependencies": {
    "innerself": "0.1.1"
  }
}
